{
    "version": 2,
    "builds": [
      {
        "src": "api/*.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.go"
      }
    ],
    "build": {
      "env": {
        "GO_BUILD_FLAGS": "-ldflags '-s -w'"
      }
    }
  }